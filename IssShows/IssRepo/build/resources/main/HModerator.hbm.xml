<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="HiberPack.HModerator"
           table="Moderator">
        <id name="ID" column="Id">
            <generator class="increment"/>
        </id>
        <property
                name="username"
                column="Username"/>
        <property
                name="passwd"
                column="Password"/>
    </class>
</hibernate-mapping>